                
           <!DOCTYPE html>
           <html lang="en">
           
           <br>
                <br>
                <br>
                <h2 class="container">Contact Us </h2>
                <hr>

				<form action="/leads" method="post" enctype="multipart/form-data">
				
                            <fieldset class="container">
                            
								<input type="hidden" name="action" value="contact_send" />

								<div class="row">
									<div class="col-md-4">
										<label for="contact:name">Full Name *</label>
										<input required type="text" value="" class="form-control"
											name="fullnameContact" id="contact:name">
									</div>
									<div class="col-md-4">
										<label for="contact:entName">Entreprise Name *</label>
										<input required type="text" value="" class="form-control"
											name="businessNameContact" id="contact:entName">
									</div>
									<div class="col-md-4">
										<label for="contact:email">E-mail Address *</label>
										<input required type="email" value="" class="form-control"
											name="emailContact" id="contact:email">
										
									</div>
									<div class="col-md-4">
										<label for="contact:phone">Phone *</label>
										<input type="text" value="" class="form-control" name="phoneContact"
											id="contact:phone">
									</div>
									<div class="col-md-8">
										<label for="contact:projectName">Project Name *</label>
										<input required type="text" value="" class="form-control"
											name="projectNameContact" id="contact:subject">
									</div>
									<div class="col-md-8">
											<label for="contact:projectName">Project Description *</label>
											<input required type="text" value="" class="form-control"
												name="projectDescContact" id="contact:subject">
										</div>
								
								
								
									<div class="col-md-4">
										<label for="contact_department">Department</label>
										<input required type="text" value="" class="form-control"
										name="department" id="contact:subject" required>										
									</div>
								</div>
								<div class="row">
									<div class="col-md-12">
										<label for="contact:message">Message *</label>
										<textarea required maxlength="10000" rows="8" class="form-control"
											name="messageContact" id="contact:message"></textarea>
									</div>
								</div>
								<div class="row">
									<div class="col-md-12">
										<label for="contact:attachment">File Attachment</label>

										<!-- custom file upload -->
										<input class="custom-file-upload" type="file" id="file"
											name="attachment" id="contact:attachment"
											data-btn-text="Select a File" />
										<small class="text-muted block">Max file size: 10Mb (zip/pdf/jpg/png)</small>

									</div>
								</div>

                <br>
                                
                                <div class="row">
								<div class="col-md-12">
                                <button type="submit" class="btn btn-primary"><i class="fa fa-check"></i> SEND
								MESSAGE</button>
								<!-- INFO -->
								<div class="col-md-4 col-sm-4 float-right">
			
									<h2>Visit Us</h2>
			
									<!-- 
										Available heights
											h-100
											h-150
